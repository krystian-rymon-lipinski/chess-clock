<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chess_clock_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chess_clock_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.krystian.chessclock.ChessClockApplication">
        <activity android:name="com.krystian.chessclock.timerPackage.TimerActivity"
            android:screenOrientation="portrait"> </activity>
        <activity android:name="com.krystian.chessclock.MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.krystian.chessclock.customMatchPackage.CustomGameActivityList"> </activity>
    </application>

</manifest>